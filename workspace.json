{
    "version": 1,
    "projects": {
        "forum-api": {
            "root": "apps/forum-api",
            "sourceRoot": "apps/forum-api/src",
            "projectType": "application",
            "prefix": "forum-api",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "options": {
                        "outputPath": "dist/apps/forum-api",
                        "main": "apps/forum-api/src/main.ts",
                        "tsConfig": "apps/forum-api/tsconfig.app.json",
                        "assets": ["apps/forum-api/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/forum-api/src/environments/environment.ts",
                                    "with": "apps/forum-api/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "forum-api:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "config": "apps/forum-api/.eslintrc",
                        "tsConfig": ["apps/forum-api/tsconfig.app.json", "apps/forum-api/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**", "!apps/forum-api/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/forum-api/jest.config.js",
                        "tsConfig": "apps/forum-api/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "backend-shared": {
            "root": "libs/backend/shared",
            "sourceRoot": "libs/backend/shared/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["libs/backend/shared/tsconfig.lib.json", "libs/backend/shared/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**", "!libs/backend/shared/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/backend/shared/jest.config.js",
                        "tsConfig": "libs/backend/shared/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "users": {
            "root": "apps/users",
            "sourceRoot": "apps/users/src",
            "projectType": "application",
            "prefix": "users",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "options": {
                        "outputPath": "dist/apps/users",
                        "main": "apps/users/src/main.ts",
                        "tsConfig": "apps/users/tsconfig.app.json",
                        "assets": ["apps/users/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/users/src/environments/environment.ts",
                                    "with": "apps/users/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "users:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:lint",
                    "options": {
                        "linter": "eslint",
                        "config": "apps/users/.eslintrc",
                        "tsConfig": ["apps/users/tsconfig.app.json", "apps/users/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**", "!apps/users/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/users/jest.config.js",
                        "tsConfig": "apps/users/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/nest"
    },
    "schematics": {
        "@nrwl/workspace": {
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
                "linter": "eslint"
            }
        },
        "@nrwl/react": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/next": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/web": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/node": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/nx-plugin": {
            "plugin": {
                "linter": "eslint"
            }
        },
        "@nrwl/nest": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/express": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "forum-api"
}
